{
  "build": {
    "beforeBuildCommand": "quasar build",
    "beforeDevCommand": "quasar dev",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/spa",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "mx-pwd",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "MX-PWD",
        "fullscreen": false,
        "height": 550,
        "width": 850,
        "center": true,
        "minHeight": 500,
        "minWidth": 800,
        "resizable": true
      }
    ]
  }
}